<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title Real Estate Project</title>
    <link rel="canonical" href="https://www.wrappixel.com/templates/ample-admin-lite/" />
    <link rel="icon" type="image/png" sizes="16x16" href="plugins/images/favicon.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/Content/dashboard/plugins/images/favicon.png" />
    <link href="~/Content/dashboard/plugins/bower_components/chartist/dist/chartist.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/dashboard/plugins/bower_components/chartist-plugin-tooltips/dist/chartist-plugin-tooltip.css">
    <link href="~/Content/dashboard/css/style.min.css" rel="stylesheet">
</head>

<body>
    <div class="click-closed"></div>
    <div class="preloader">
        <div class="lds-ripple">
            <div class="lds-pos"></div>
            <div class="lds-pos"></div>
        </div>
    </div>
    <div id="main-wrapper" data-layout="vertical" data-navbarbg="skin5" data-sidebartype="full"
         data-sidebar-position="absolute" data-header-position="absolute" data-boxed-layout="full">
        <header class="topbar" data-navbarbg="skin5">
            <nav class="navbar top-navbar navbar-expand-md navbar-dark">
                <div class="navbar-header" data-logobg="skin6">
                    <a class="navbar-brand" href="dashboard.html">
                        <b class="logo-icon">
                            <img src="~/Content/dashboard/plugins/images/logo-icon.png" alt="homepage" />

                        </b>
                        <span class="logo-text">
                            <img src="~/Content/dashboard/plugins/images/logo-text.png" alt="homepage" />

                        </span>
                    </a>
                    <a class="nav-toggler waves-effect waves-light text-dark d-block d-md-none"
                       href="javascript:void(0)"><i class="ti-menu ti-close"></i></a>
                </div>
                <div class="navbar-collapse collapse" id="navbarSupportedContent" data-navbarbg="skin5">
                    <ul class="navbar-nav d-none d-md-block d-lg-none">
                        <li class="nav-item">
                            <a class="nav-toggler nav-link waves-effect waves-light text-white"
                               href="javascript:void(0)"><i class="ti-menu ti-close"></i></a>
                        </li>
                    </ul>
                    <ul class="navbar-nav ms-auto d-flex align-items-center">
                        <li class=" in">
                            <form role="search" class="app-search d-none d-md-block me-3">
                                <input type="text" placeholder="Search..." class="form-control mt-0">
                                <a href="" class="active">
                                    <i class="fa fa-search"></i>
                                </a>
                            </form>
                        </li>
                        <li>
                            <a class="profile-pic" href="#">
                                <img src="plugins/images/users/varun.jpg" alt="user-img" width="36"
                                     class="img-circle"><span class="text-white font-medium">admin</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
    </div>
    <aside class="left-sidebar" data-sidebarbg="skin6">
        <div class="scroll-sidebar">
            <nav class="sidebar-nav">
                <ul id="sidebarnav">
                    <li class="sidebar-item pt-2">
                        <a class="sidebar-link waves-effect waves-dark sidebar-link text-dark" href="http://localhost:44348/Home/Dashboard"
                           aria-expanded="false">
                            <i class="far fa-clock text-black" aria-hidden="true"></i>
                            <span class="hide-menu">Dashboard</span>
                        </a>
                    </li>
                    <!--    <li class="sidebar-item">
    <a class="sidebar-link waves-effect waves-dark sidebar-link text-dark" href="http://localhost:44348/Home/Data"
       aria-expanded="false">
        <i class="fa fa-table" aria-hidden="true"></i>
        <span class="hide-menu">Booking Orders</span>
    </a>
            </li>-->
                    <li class="sidebar-item">
                        <a class="sidebar-link waves-effect waves-dark sidebar-link text-dark" href="http://localhost:44348/Home/CreateProperty"
                           aria-expanded="false">
                            <i class="fa fa-edit" aria-hidden="true"></i>
                            <span class="hide-menu">Create Property</span>
                        </a>
                    </li>
                    <li class="sidebar-item">
                        <a class="sidebar-link waves-effect waves-dark sidebar-link text-dark" href="http://localhost:44348/Home/ActiveProperties"
                           aria-expanded="false">
                            <i class="fa fa-globe" aria-hidden="true"></i>
                            <span class="hide-menu text-black">Active Properties</span>
                        </a>
                    </li>
                    <li class="sidebar-item">
                        <a class="sidebar-link waves-effect waves-dark sidebar-link text-dark" href="http://localhost:44348/Home/ShowOrder"
                           aria-expanded="false">
                            <i class="fa fa-user" aria-hidden="true"></i>
                            <span class="hide-menu">Orders</span>
                        </a>
                    </li>
                    <li class="sidebar-item">
                        <a class="sidebar-link waves-effect waves-dark sidebar-link text-dark" href="http://localhost:44348/Home/RegisteredCustomer"
                           aria-expanded="false">
                            <i class="fa fa-user" aria-hidden="true"></i>
                            <span class="hide-menu">Registered Customer</span>
                        </a>
                    </li>
                    <li class="sidebar-item">
                        <a class="sidebar-link waves-effect waves-dark sidebar-link text-dark" href="@Url.Action("Logout", "Home")">
                            <i class="fa fa-columns" aria-hidden="true"></i>
                            <span class="hide-menu">Logout</span>
                        </a>
                    </li>
                </ul>
            </nav>
        </div>
    </aside>

    <div class="container-fluid page-breadcrumb body-content m-0 p-0">
        @RenderBody()
        <hr />
    </div>
    <footer class="footer text-center">
        2023 © Ample Admin brought to you by <a href="https://www.wrappixel.com/">universaltechonologies.tech</a>
    </footer>
    <script src="~/Content/dashboard/plugins/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="~/Content/dashboard/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/dashboard/js/app-style-switcher.js"></script>
    <script src="~/Content/dashboard/plugins/bower_components/jquery-sparkline/jquery.sparkline.min.js"></script>
    <script src="~/Content/dashboard/js/waves.js"></script>
    <script src="~/Content/dashboard/js/sidebarmenu.js"></script>
    <script src="~/Content/dashboard/js/custom.js"></script>
    <script src="~/Content/dashboard/plugins/bower_components/chartist/dist/chartist.min.js"></script>
    <script src="~/Content/dashboard/plugins/bower_components/chartist-plugin-tooltips/dist/chartist-plugin-tooltip.min.js"></script>
    <script src="~/Content/dashboard/js/pages/dashboards/dashboard1.js"></script>
</body>
</html>